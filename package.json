{
  "name": "kui-vue",
  "version": "3.4.0",
  "description": "A high quality UI Toolkit built on Vue.js 2.0",
  "private": false,
  "main": "dist/k-ui.js",
  "style": "dist/k-ui.css",
  "files": [
    "dist",
    "test",
    "components/"
  ],
  "scripts": {
    "start": "npm run dev",
    "buildlib": "webpack --config build/webpack.config.js --env prod",
    "builddocs": "webpack --config build/webpack.config.js --env docs",
    "buildlang": "webpack --config build/webpack.config.js --env lang",
    "dev": "vue-cli-service serve",
    "builddocs_bak": "vue-cli-service build --dest docs",
    "buildlib_bak": "vue-cli-service build --progres --no-clean --target lib --name k-ui components/bin/index.js",
    "buildlang_bak": "vue-cli-service build --progres --dest dist/locale --target lib --name lang --formats umd-min components/locale/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/chuchur/kui-vue"
  },
  "author": {
    "name": "chuchur",
    "email": "chuchur@qq.com"
  },
  "license": "ISC",
  "keywords": [
    "vue",
    "vue3",
    "kui",
    "kyui",
    "design",
    "kui-vue",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "homepage": "https://k-ui.cn",
  "bugs": {
    "url": "https://gitee.com/chuchur/kui-vue/issues",
    "email": "chuchur@qq.com"
  },
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "dayjs": "^1.11.8",
    "kui-icons": "^4.1.3",
    "kui-loader": "^1.0.6",
    "lodash.assign": "^4.2.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "core-js": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-loader": "^6.2.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^3.0.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "vue": "^2.7.16",
    "vue-clipboard2": "^0.3.3",
    "vue-i18n": "^8.28.2",
    "vue-loader": "^15.11.1",
    "vue-router": "^3.6.5",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^5.1.4",
    "webpackbar": "^6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not dead"
  ]
}
